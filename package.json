{
  "name": "vlist.dev",
  "version": "0.2.0",
  "description": "vlist.dev â€” documentation, sandbox, and API for the vlist library",
  "type": "module",
  "imports": {
    "vlist": "@floor/vlist",
    "vlist/*": "@floor/vlist/*"
  },
  "scripts": {
    "dev": "bun run build:sandbox && bun run build:bench && bun run --watch server.ts",
    "start": "bun run server.ts",
    "build:sandbox": "bun run sandbox/build.ts",
    "build:sandbox:watch": "bun run sandbox/build.ts --watch",
    "build:bench": "bun run benchmarks/build.ts",
    "build:bench:watch": "bun run benchmarks/build.ts --watch",
    "typecheck": "tsc --noEmit",
    "link:libs": "cd ~/Code/floor/vlist && bun link && cd ~/Code/floor/vlist.dev && bun link vlist"
  },
  "dependencies": {
    "@floor/vlist": "file:../vlist",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "marked": "^17.0.1",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "vlist-react": "^0.1.1",
    "vlist-svelte": "^0.1.1",
    "vlist-vue": "^0.1.1",
    "vue": "^3.5.28"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "sass": "^1.85.0",
    "typescript": "^5.8.0"
  }
}
